{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": ["src/**/*.ts"],
  "exclude": ["src/**/*.test.ts"],
  "check-coverage": true,
  "branches": 70,
  "lines": 70,
  "functions": 70,
  "statements": 70,
  "ignore-class-method": ["render"],
  "extension": [".js", ".ts"],
  "reporter": ["text", "text-summary", "lcov", "json"],
  "report-dir": "./test-reports/all/coverage",
  "temp-dir": "./test-reports/all/.nyc_output"
}
